! Program to explain the numbers and data types in Fortran

program numbers

    implicit none
    ! l, m, n .... Integers - do not have a fraction
    ! a, b, c, x, y, z ... Real/Floating

    integer a, b, c
    real ar, br, cr
    double ad, bd, cd
    complex ac, bc, cc
    !(4+5i, 7-9.3j)
    character ach, bch, cch,
    logical al, bl, cl

end program numbers
